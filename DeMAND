library(DeMAND)
Exp <- read.csv("XL147Exp.csv", header = TRUE, sep = ",", stringsAsFactors = FALSE)
ExpMat <- as.matrix(Exp)
str(ExpMat)
Anno <- read.csv("XL147Anno.csv", header = TRUE, sep = ",", stringsAsFactors = FALSE)
AnnoMat <- as.matrix(Anno)
str(AnnoMat)
Net <- read.csv("CFTRstringNetwork.csv", header = TRUE, sep = ",", stringsAsFactors = FALSE)
NetMat <- as.matrix(Net)
str(NetMat)
XLcaseIndex <- c(1:4)
XLcontrolIndex <- c(5:8)
dobj <- demandClass(exp=ExpMat, anno=AnnoMat, network=NetMat)
printDeMAND(dobj)
dobj <- runDeMAND(dobj, fgIndex=XLcaseIndex, bgIndex=XLcontrolIndex)
printDeMAND(dobj)
